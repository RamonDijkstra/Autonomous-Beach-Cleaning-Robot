@prefix ex: <http://example.com/bachelor_project/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:animal ex:action_required ex:move_around ;
    ex:has_property ex:can_move,
        ex:can_walk,
        ex:dynamic ;
    rdfs:subClassOf ex:dynamic_object,
        ex:object .

ex:ball ex:action_required ex:move_around ;
    ex:has_property ex:can_move,
        ex:dynamic ;
    rdfs:subClassOf ex:dynamic_object,
        ex:object,
        ex:plastic_object,
        ex:static_object .

ex:boat ex:action_required ex:move_around ;
    ex:has_position ex:on_the_ground ;
    rdfs:subClassOf ex:object,
        ex:static_object .

ex:bottle ex:action_required ex:pick_up ;
    ex:has_weight ex:low ;
    ex:pick_up_ability ex:one_arm ;
    rdfs:subClassOf ex:plastic_object,
        ex:static_object .

ex:car ex:action_required ex:move_around ;
    ex:has_property ex:can_move,
        ex:dynamic ;
    rdfs:subClassOf ex:dynamic_object,
        ex:object .

ex:chair ex:action_required ex:move_around ;
    ex:has_position ex:on_the_ground ;
    rdfs:subClassOf ex:object,
        ex:static_object .

ex:inflatables ex:action_required ex:move_around ;
    ex:contains ex:air ;
    rdfs:subClassOf ex:object,
        ex:plastic_object,
        ex:static_object .

ex:person ex:action_required ex:move_around ;
    ex:has_property ex:can_move,
        ex:can_walk,
        ex:dynamic ;
    rdfs:subClassOf ex:dynamic_object,
        ex:object .

ex:plastic_bag ex:action_required ex:pick_up ;
    ex:pick_up_ability ex:one_arm ;
    rdfs:subClassOf ex:object,
        ex:plastic_object,
        ex:static_object .

ex:plastic_sack ex:action_required ex:pick_up ;
    ex:pick_up_ability ex:two_arms ;
    rdfs:subClassOf ex:plastic_object,
        ex:static_object .

ex:pole rdfs:subClassOf ex:object,
        ex:static_object .

ex:robot ex:bin_location "-8.659522, 115.130313" ;
    ex:saved_location "-8.659655, 115.130383" ;
    rdfs:subClassOf ex:object ;
    geo:lat "-8.659589" ;
    geo:long "115.130358" .

ex:sunbed rdfs:subClassOf ex:object,
        ex:static_object .

ex:towel ex:action_required ex:move_around ;
    ex:has_position ex:on_the_ground ;
    rdfs:subClassOf ex:object,
        ex:static_object .

ex:tree ex:action_required ex:move_around ;
    ex:has_property ex:leaves ;
    rdfs:subClassOf ex:object,
        ex:static_object .

ex:umbrella ex:action_required ex:move_around ;
    ex:has_position ex:on_the_ground ;
    rdfs:subClassOf ex:object,
        ex:static_object .

ex:windscreen ex:action_required ex:move_around ;
    ex:has_position ex:on_the_ground ;
    rdfs:subClassOf ex:object,
        ex:static_object .

ex:dynamic_object rdfs:subClassOf ex:object .

ex:plastic_object rdfs:subClassOf ex:object,
        ex:static_object .

ex:static_object rdfs:subClassOf ex:object .

